{
  "name": "@fabled/app",
  "private": true,
  "version": "1.4.0",
  "type": "module",
  "scripts": {
    "act": "act -j ios --env-file .env --secret-file .secrets --var-file .env -P macos-latest=-self-hosted",
    "assets:ios": "capacitor-assets generate --ios",
    "assets:android": "capacitor-assets generate --android",
    "build": "tsc && vite build",
    "erase": "xcrun simctl erase all",
    "gen:admin": "graphql-codegen --config ./api.codegen.ts && add-ts-nocheck --patterns 'src/_network/admin/__generated__/*.ts'",
    "gen:api": "orval && add-ts-nocheck --patterns 'src/_network/api/__generated__/*.ts'",
    "ios": "ionic cap run ios -l --external",
    "android": "ionic cap run android -l --external",
    "lint": "eslint ./src --max-warnings=0 --fix",
    "preview": "vite preview",
    "start": "vite"
  },
  "dependencies": {
    "@capacitor-community/in-app-review": "^6.0.0",
    "@capacitor-firebase/authentication": "^6.1.0",
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "6.0.0",
    "@capacitor/browser": "^6.0.2",
    "@capacitor/core": "6.1.1",
    "@capacitor/device": "^6.0.1",
    "@capacitor/haptics": "6.0.0",
    "@capacitor/keyboard": "6.0.1",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "6.0.0",
    "@capawesome/capacitor-app-update": "^6.0.0",
    "@emotion/react": "^11.14.0",
    "@ionic/core": "^8.2.6",
    "@ionic/react": "^8.0.0",
    "@ionic/react-router": "^8.0.0",
    "@react-spring/web": "^9.7.4",
    "@revenuecat/purchases-capacitor": "^9.0.3",
    "@rudderstack/analytics-js": "^3.11.1",
    "@sentry/react": "^8.34.0",
    "@tanstack/react-query": "^5.51.15",
    "axios": "^1.7.4",
    "classnames": "^2.5.1",
    "firebase": "^10.12.4",
    "i18next": "^23.12.2",
    "ionicons": "^7.0.0",
    "lodash": "^4.17.21",
    "micromustache": "^8.0.3",
    "qs": "^6.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
